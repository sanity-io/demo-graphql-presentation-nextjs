    "@sanity/preview-url-secret": "1.6.7",
    "@sanity/vision": "3.36.4",
    "@sanity/visual-editing": "1.8.7",
    "@tailwindcss/typography": "0.5.12",
    "@types/node": "^20.11.26",
    "@types/react": "18.2.74",
    "@types/react-dom": "18.2.24",
    "@urql/core": "5.0.0",
    "@urql/next": "1.1.1",
    "@vercel/speed-insights": "1.0.10",
    "autoprefixer": "10.4.19",
    "date-fns": "3.6.0",
    "graphql": "16.8.1",
    "next": "14.1.4",
    "next-sanity": "8.5.3",
    "next-urql": "5.0.2",
    "postcss": "8.4.38",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rxjs": "7.8.1",
    "sanity": "3.36.4",
    "sanity-plugin-asset-source-unsplash": "2.0.1",
    "styled-components": "6.1.8",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.2",
    "urql": "4.0.7",
    "wonka": "6.3.4"
