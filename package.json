{
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next",
    "format": "prettier --cache --write .",
    "graphql:deploy": "sanity graphql deploy",
    "lint": "next lint .",
    "presetup": "echo 'about to setup env variables, follow the guide here: https://github.com/sanity-io/demo-graphql-presentation-nextjs#using-the-sanity-cli'",
    "setup": "npx sanity@latest init --env .env.local",
    "postsetup": "echo 'create the read token by following the rest of the guide: https://github.com/sanity-io/demo-graphql-presentation-nextjs#creating-a-read-token'",
    "start": "next start"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-packagejson",
      "prettier-plugin-tailwindcss"
    ],
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "@sanity/assist": "3.0.8",
    "@sanity/client": "6.22.2",
    "@sanity/icons": "2.11.8",
    "@sanity/image-url": "1.0.2",
    "@sanity/preview-url-secret": "1.6.21",
    "@sanity/vision": "3.62.2",
    "@sanity/visual-editing": "1.8.22",
    "@tailwindcss/typography": "0.5.15",
    "@types/node": "^20.11.26",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@urql/core": "5.0.8",
    "@urql/next": "1.1.2",
    "@vercel/speed-insights": "1.0.13",
    "autoprefixer": "10.4.20",
    "date-fns": "3.6.0",
    "graphql": "16.9.0",
    "next": "14.2.16",
    "next-sanity": "9.8.7",
    "next-urql": "5.0.2",
    "postcss": "8.4.47",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rxjs": "7.8.1",
    "sanity": "3.62.2",
    "sanity-plugin-asset-source-unsplash": "3.0.1",
    "styled-components": "6.1.13",
    "tailwindcss": "3.4.14",
    "typescript": "5.6.3",
    "urql": "4.2.0",
    "wonka": "6.3.4"
  },
  "devDependencies": {
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.16",
    "prettier": "3.3.3",
    "prettier-plugin-packagejson": "2.5.3",
    "prettier-plugin-tailwindcss": "0.6.8"
  },
  "packageManager": "pnpm@9.1.1"
}
